{
  "name": "harperdb-node-client",
  "version": "1.0.5",
  "description": "High-performance HarperDB client for Node.js/TypeScript with connection pooling (keep-alive), query caching with auto-invalidation, bulk/parallel APIs, and GraphQL schema apply.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "harperdb",
    "database",
    "nosql",
    "sql",
    "client",
    "sdk",
    "driver",
    "optimized",
    "parallel",
    "bulk",
    "upsert",
    "caching",
    "keep-alive",
    "node",
    "nodejs",
    "javascript",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/loc3pro/harperdb-node-client"
  },
  "bugs": {
    "url": "https://github.com/loc3pro/harperdb-node-client/issues"
  },
  "homepage": "https://github.com/loc3pro/harperdb-node-client#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
